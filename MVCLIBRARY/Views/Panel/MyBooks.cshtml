@using MVCLIBRARY.Models.Entity
@model List<TBL_DEPOSIT>
@{
    ViewBag.Title = "MyBooks";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}
<h4>Kitap Listem</h4>
<!DOCTYPE html>

<html>

<body class="hold-transition sidebar-mini">

    <section class="content">

        <div class="container-fluid">

            <div class="row">



                <!-- /.col -->

                <div class="col-md-12">

                    <div class="card">

                        <div class="card-header p-2">



                        </div><!-- /.card-header -->

                        <div class="card-body">



                            <div class="tab-pane" id="timeline">

                                <!-- The timeline -->

                                <div class="timeline timeline-inverse">

                                    <!-- timeline time label -->

                                    @foreach (var item in Model)
                                    {
                                        <div class="time-label">
                                            <a class="btn btn-success">
                                                <span class="bg-success">
                                                    @(((DateTime)item.STARTTIME).ToString("dd-MMM-yyyy"))
                                                </span>
                                            </a>
                                            <a class="btn btn-warning">
                                                <span class="bg-warning">
                                                    @(((DateTime)item.USERGETTIME).ToString("dd-MMM-yyyy"))
                                                </span>
                                            </a>
                                        </div>
                                        <div>
                                            <div class="timeline-item">
                                                <div class="timeline-body">
                                                    Kitap Adı: @item.TBL_BOOK.NAME 
                                                    <br />
                                                    Yazarı: @item.TBL_BOOK.TBL_AUTHOR.NAME @item.TBL_BOOK.TBL_AUTHOR.SURNAME
                                                </div>
                                            </div>
                                        </div>
                                    }

                                </div>

                               
                            </div>

                        </div>

                    </div><!-- /.card-body -->

                </div>

                <!-- /.nav-tabs-custom -->

            </div>

            <!-- /.col -->

        </div>

    </section>

</body>

</html>



